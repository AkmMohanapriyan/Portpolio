{
  "version": 2,
  "builds": [
    {
      "src": "Back-End/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["Back-End/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "dest": "Back-End/server.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/.*",
      "status": 404
    }
  ]
}